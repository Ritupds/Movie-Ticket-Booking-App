           class=\"form-control\"\r\n                v-model=\"searchQuery\"\r\n                @input=\"search\"\r\n                placeholder=\"Search venues and shows\"\r\n              />\r\n            </div>\r\n\r\n            <!-- Shows Table -->\r\n            <div class=\"table-responsive\">\r\n              <h3 class=\"text-center\">Shows</h3>\r\n              <table class=\"table table-bordered\">\r\n                <thead>\r\n                  <tr>\r\n                    <th>Name</th>\r\n                    <th>Date</th>\r\n                    <th>Time</th>\r\n                    <th>available_seats</th>\r\n                    <th>Venue Name</th>\r\n                    <th>Venue City</th>\r\n                    <th>Action</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"show in shows\" :key=\"show.id\">\r\n                    <td>{{ show.show_name }}</td>\r\n                    <td>{{ show.date }}</td>\r\n                    <td>{{ show.time }}</td>\r\n                    <td>{{ show.available_seats }}</td>\r\n                    <td>{{ show.venue.name }}</td>\r\n                    <td>{{ show.venue.city }}</td>\r\n                    <td>\r\n                      <router-link :to=\"'/book_show/' + show.id\"\r\n                        >Book Show</router-link\r\n                      >\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-for=\"booking in bookings\" :key=\"booking.id\">\r\n      <h2>Booking Information</h2>\r\n      <p>ID: {{ booking.id }}</p>\r\n      <p>Show ID: {{ booking.show_id }}</p>\r\n      <p>Number of Seats: {{ booking.number_of_seats }}</p>\r\n\r\n      <div v-if=\"showDetails[booking.show_id]\">\r\n        <p>Show Name: {{ showDetails[booking.show_id].show_name }}</p>\r\n        <p>Show Date: {{ showDetails[booking.show_id].date }}</p>\r\n        <p>Show Time: {{ showDetails[booking.show_id].time }}</p>\r\n      </div>\r\n\r\n      <div v-else>\r\n        <p>No show information available for this booking.</p>\r\n      </div>\r\n    </div>\r\n\r\n    <br />\r\n    <br />\r\n    <button @click=\"logout\" class=\"btn btn-danger\">Logout</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      token: \"\",\r\n      venues: [],\r\n      shows: [],\r\n      searchQuery: \"\",\r\n      booking_list:[],\r\n      booking: null,\r\n      bookings: [],\r\n      showDetails: {},\r\n    };\r\n  },\r\n  computed:{\r\n    axiosConfig(){\r\n      return {\r\n        headers: {\r\n          Authorization: `Bearer ${this.token}`,\r\n        }\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    let token = localStorage.getItem(\"access_token\");\r\n    if (token == null){\r\n      this.$router.push(\"/user_login\");\r\n    } else {\r\n      this.token = token;\r\n    }\r\n    this.fetchVenues();\r\n    this.fetchShows();\r\n    this.fetchBookings();\r\n  },\r\n  methods: {\r\n    fetchVenues() {\r\n      axios\r\n        .get(\"http://127.0.0.1:5000/admindashboard/venues\")\r\n        .then((response) => {\r\n          this.venues = response.data.venues;\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error fetching venues:\", error);\r\n        });\r\n    },\r\n    fetchShows() {\r\n      axios\r\n        .get(\"http://127.0.0.1:5000/admindashboard/shows\")\r\n        .then((response) => {\r\n          this.shows = response.data.shows;\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error fetching shows:\", error);\r\n        });\r\n    },\r\n    fetchShowDetails(showId) {\r\n      axios\r\n        .get(`http://127.0.0.1:5000/admindashboard/shows/${showId}`)\r\n        .then((response) => {\r\n          this.$set(this.showDetails, showId, response.data);\r\n        })\r\n        .catch((error) => {\r\n          console.error(`Error fetching show details for show ID ${showId}:`, error);\r\n        });\r\n    },\r\n    fetchBookings() {\r\n      axios\r\n        .get(\"http://127.0.0.1:5000/bookings\")\r\n        .then((response) => {\r\n          this.bookings = response.data.bookings;\r\n          this.bookings.forEach((booking) => {\r\n            this.fetchShowDetails(booking.show_id);\r\n          });\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error fetching bookings:\", error);\r\n        });\r\n    },\r\n    search() {\r\n      axios.get(`http://127.0.0.1:5000/search/venues?query=${this.searchQuery}`)\r\n        .then((response) => {\r\n          this.venues = response.data.venues;\r\n        })\r\n        .catch((error) => {\r\n          console.error('Error while searching venues:', error);\r\n        });\r\n\r\n      axios.get(`http://127.0.0.1:5000/search/shows?query=${this.searchQuery}`)\r\n        .then((response) => {\r\n          this.shows = response.data.shows;\r\n        })\r\n        .catch((error) => {\r\n          console.error('Error while searching shows:', error);\r\n        });\r\n    },\r\n    bookShow(show) {\r\n      this.$router.push({ name: 'BookShow', params: { showId: show.id } });\r\n    },\r\n    logout() {\r\n      this.$router.push({ name: \"UserLogin\" });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAyFA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,MAAA;MACAC,KAAA;MACAC,WAAA;MACAC,YAAA;MACAC,OAAA;MACAC,QAAA;MACAC,WAAA;IACA;EACA;EACAC,QAAA;IACAC,YAAA;MACA;QACAC,OAAA;UACAC,aAAA,iBAAAX,KAAA;QACA;MACA;IACA;EACA;EACAY,QAAA;IACA,IAAAZ,KAAA,GAAAa,YAAA,CAAAC,OAAA;IACA,IAAAd,KAAA;MACA,KAAAe,OAAA,CAAAC,IAAA;IACA;MACA,KAAAhB,KAAA,GAAAA,KAAA;IACA;IACA,KAAAiB,WAAA;IACA,KAAAC,UAAA;IACA,KAAAC,aAAA;EACA;EACAC,OAAA;IACAH,YAAA;MACAnB,KAAA,CACAuB,GAAA,gDACAC,IAAA,CAAAC,QAAA;QACA,KAAAtB,MAAA,GAAAsB,QAAA,CAAAxB,IAAA,CAAAE,MAAA;MACA,GA