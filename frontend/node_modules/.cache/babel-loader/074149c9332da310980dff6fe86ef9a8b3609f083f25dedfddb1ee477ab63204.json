able class=\"table table-bordered\">\r\n                <thead>\r\n                  <tr>\r\n                    <th>Name</th>\r\n                    <th>City</th>\r\n                    <th>Place</th>\r\n                    <th>Capacity</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"venue in venues\" :key=\"venue.id\">\r\n                    <td>{{ venue.name }}</td>\r\n                    <td>{{ venue.city }}</td>\r\n                    <td>{{ venue.place }}</td>\r\n                    <td>{{ venue.capacity }}</td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div> -->\r\n\r\n  \r\n              <!-- Shows Table -->\r\n              <div class=\"table-responsive\">\r\n                <h3 class=\"text-center\">Shows</h3>\r\n                <table class=\"table table-bordered\">\r\n                  <thead>\r\n                    <tr>\r\n                      <th>Name</th>\r\n                      <th>Date</th>\r\n                      <th>Time</th>\r\n                      <th>available_seats</th>\r\n                      <th>Venue Name</th>\r\n                      <th>Venue City</th>\r\n                      <th>Action</th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody>\r\n                    <tr v-for=\"show in shows\" :key=\"show.id\">\r\n                      <td>{{ show.show_name }}</td>\r\n                      <td>{{ show.date }}</td>\r\n                      <td>{{ show.time }}</td>\r\n                      <td>{{ show.available_seats }}</td>\r\n                      <td>{{ show.venue.name }}</td>\r\n                      <td>{{ show.venue.city }}</td>\r\n                      <!-- <td><button class=\"btn btn-primary\" @click=\"bookShow(show)\">Book</button></td> -->\r\n                      <!-- <td><button type=\"button\" class=\"btn btn-outline-light\">\r\n        <router-link to=\"/book_show\">Book</router-link>\r\n      </button></td> -->\r\n                      <td><router-link :to=\"'/book_show/' + show.id\">Book Show</router-link></td>\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      \r\n      \r\n   \r\n    <div v-for=\"booking in bookings\" :key=\"booking.id\">\r\n  <h2>Booking Information</h2>\r\n  <p>ID: {{ booking.id }}</p>\r\n  <p>Show ID: {{ booking.show_id }}</p>\r\n  <p>Number of Seats: {{ booking.number_of_seats }}</p>\r\n\r\n  <!-- Display show information if available -->\r\n  <div v-if=\"showDetails[booking.show_id]\">\r\n    <p>Show Name: {{ showDetails[booking.show_id].show_name }}</p>\r\n    <p>Show Date: {{ showDetails[booking.show_id].date }}</p>\r\n    <p>Show Time: {{ showDetails[booking.show_id].time }}</p>\r\n    \r\n\r\n    <!-- Add more show details as needed -->\r\n  </div>\r\n\r\n  <div v-else>\r\n    <p>No show information available for this booking.</p>\r\n  </div>\r\n</div>\r\n  \r\n\r\n   \r\n\r\n    <br> <br> \r\n      <button @click=\"logout\" class=\"btn btn-danger\">Logout</button>\r\n    </div>\r\n  </template>\r\n\r\n\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      venues: [],\r\n      shows: [],\r\n      searchQuery: \"\",\r\n      booking_list:[],\r\n      booking: null,\r\n      bookings: [],\r\n      showDetails: {},\r\n      \r\n    \r\n    };\r\n  },\r\n  created() {\r\n    \r\n    this.fetchVenues();\r\n    this.fetchShows();\r\n    this.fetchBookings();\r\n  },\r\n  methods: {\r\n    fetchVenues() {\r\n      \r\n      axios\r\n        .get(\"http://localhost:5000/admindashboard/venues\") \r\n        .then((response) => {\r\n          this.venues = response.data.venues;\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error fetching venues:\", error);\r\n        });\r\n    },\r\n    fetchShows() {\r\n      \r\n      axios\r\n        .get(\"http://127.0.0.1:5000/admindashboard/shows\")\r\n        .then((response) => {\r\n          this.shows = response.data.shows;\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error fetching shows:\", error);\r\n        });\r\n    },\r\n\r\n\r\n    fetchShowDetails(showId) {\r\n    axios\r\n      .get(`http://127.0.0.1:5000/admindashboard/shows/${showId}`)\r\n      .then((response) => {\r\n        this.$set(this.showDetails, showId, response.data); // Store show details using showId as the key\r\n      })\r\n      .catch((error) => {\r\n        console.error(`Error fetching show details for show ID ${showId}:`, error);\r\n      });\r\n  },\r\n\r\nfetchBookings() {\r\n  axios\r\n    .get(\"http://127.0.0.1:5000/bookings\")\r\n    .then((response) => {\r\n      this.bookings = response.data.bookings;\r\n      // Fetch show details for each booking\r\n      this.bookings.forEach((booking) => {\r\n        this.fetchShowDetails(booking.show_id);\r\n      });\r\n    })\r\n    .catch((error) => {\r\n      console.error(\"Error fetching bookings:\", error);\r\n    });\r\n  },\r\n\r\n\r\n\r\n\r\n\r\n   \r\n    search() {\r\n    \r\n      axios.get(`127.0.0.1:5000/search/venues?query=${this.searchQuery}`)\r\n        .then((response) => {\r\n          this.venues = response.data.venues;\r\n        })\r\n        .catch((error) => {\r\n          console.error('Error while searching venues:', error);\r\n        });\r\n\r\n      \r\n      axios.get(`127.0.0.1:5000/search/shows?query=${this.searchQuery}`)\r\n        .then((response) => {\r\n          this.shows = response.data.shows;\r\n        })\r\n        .catch((error) => {\r\n          console.error('Error while searching shows:', error);\r\n        });\r\n    },\r\n    bookShow(show) {\r\n  \r\n  this.$router.push({ name: 'BookShow', params: { showId: show.id } });\r\n},\r\n\r\n    logout() {\r\n     \r\n      this.$router.push({ name: \"UserLogin\" });\r\n    },\r\n\r\n\r\n \r\n  },\r\n};\r\n</script>"],"mappings":"AAuHA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,KAAA;MACAC,WAAA;MACAC,YAAA;MACAC,OAAA;MACAC,QAAA;MACAC,WAAA;IAGA;EACA;EACAC,QAAA;IAEA,KAAAC,WAAA;IACA,KAAAC,UAAA;IACA,KAAAC,aAAA;EACA;EACAC,OAAA;IACAH,YAAA;MAEAV,KAAA,CACAc,GAAA,gDACAC,IAAA,CAAAC,QAAA;QACA,KAAAd,MAAA,GAAAc,QAAA,CAAAf,IAAA,CAAAC,MAAA;MACA,GACAe,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,2BAAAA,KAAA;MACA;IACA;IACAP,WAAA;MAEAX,KAAA,CACAc,GAAA,+CACAC,IAAA,CAAAC,QAAA;QACA,KAAAb,KAAA,GAAAa,QAAA,CAAAf,IAAA,CAAAE,KAAA;MACA,GACAc,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,0BAAAA,KAAA;MACA;IACA;IAGAE,iBAAAC,MAAA;MACArB,KAAA,CACAc,GAAA,+CAAAO,MAAA,IACAN,IAAA,CAAAC,QAAA;QACA,KAAAM,IAAA,MAAAd,WAAA,EAAAa,MAAA,EAAAL,QAAA,CAAAf,IAAA;MACA,GACAgB,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,4CAAAG,MAAA,KAAAH,KAAA;MACA;IACA;IAEAN,cAAA;MACAZ,KAAA,CACAc,GAAA,mCACAC,IAAA,CAAAC,QAAA;QACA,KAAAT,QAAA,GAAAS,QAAA,CAAAf,IAAA,CAAAM,QAAA;QACA;QACA,KAAAA,QAAA,CAAAgB,OAAA,CAAAjB,OAAA;UACA,KAAAc,gBAAA,CAAAd,OAAA,CAAAkB,OAAA;QACA;MACA,GACAP,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,6BAAAA,KAAA;MACA;IACA;IAOAO,OAAA;MAEAzB,KAAA,CAAAc,GAAA,4CAAAV,WAAA,IACAW,IAAA,CAAAC,QAAA;QACA,KAAAd,MAAA,GAAAc,QAAA,CAAAf,IAAA,CAAAC,MAAA;MACA,GACAe,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,kCAAAA,KAAA;MACA;MAGAlB,KAAA,CAAAc,GAAA,2CAAAV,WAAA,IACAW,IAAA,CAAAC,QAAA;QACA,KAAAb,KAAA,GAAAa,QAAA,CAAAf,IAAA,CAAAE,KAAA;MACA,GACAc,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,iCAAAA,KAAA;MACA;IACA;IACAQ,SAAAC,IAAA;MAEA,KAAAC,OAAA,CAAAC,IAAA;QAAAC,IAAA;QAAAC,MAAA;UAAAV,MAAA,EAAAM,IAAA,CAAAK;QAAA;MAAA;IACA;IAEAC,OAAA;MAEA,KAAAL,OAAA,CAAAC,IAAA;QAAAC,IAAA;MAAA;IACA;EAIA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}