{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      venues: [],\n      shows: [],\n      searchQuery: \"\",\n      booking_list: [],\n      booking: null,\n      bookings: [],\n      showDetails: {}\n    };\n  },\n  created() {\n    this.fetchVenues();\n    this.fetchShows();\n    this.fetchBookings();\n  },\n  methods: {\n    fetchVenues() {\n      axios.get(\"http://localhost:5000/admindashboard/venues\").then(response => {\n        this.venues = response.data.venues;\n      }).catch(error => {\n        console.error(\"Error fetching venues:\", error);\n      });\n    },\n    fetchShows() {\n      axios.get(\"http://127.0.0.1:5000/admindashboard/shows\").then(response => {\n        this.shows = response.data.shows;\n      }).catch(error => {\n        console.error(\"Error fetching shows:\", error);\n      });\n    },\n    fetchShowDetails(showId) {\n      axios.get(`http://127.0.0.1:5000/admindashboard/shows/${showId}`).then(response => {\n        this.$set(this.showDetails, showId, response.data); // Store show details using showId as the key\n      }).catch(error => {\n        console.error(`Error fetching show details for show ID ${showId}:`, error);\n      });\n    },\n    fetchBookings() {\n      axios.get(\"http://127.0.0.1:5000/bookings\").then(response => {\n        this.bookings = response.data.bookings;\n        // Fetch show details for each booking\n        this.bookings.forEach(booking => {\n          this.fetchShowDetails(booking.show_id);\n        });\n      }).catch(error => {\n        console.error(\"Error fetching bookings:\", error);\n      });\n    },\n    search() {\n      axios.get(`127.0.0.1:5000/search/venues?query=${this.searchQuery}`).then(response => {\n        this.venues = response.data.venues;\n      }).catch(error => {\n        console.error('Error while searching venues:', error);\n      });\n      axios.get(`127.0.0.1:5000/search/shows?query=${this.searchQuery}`).then(response => {\n 